<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.justin.ms5.MainMenuActivity">

    <TextView android:text="TUG-O-WAR" android:layout_width="300dp"
        android:layout_height="150dp"
        android:id="@+id/textView7"
        android:textSize="90sp"
        android:gravity="center_vertical|center_horizontal"
        android:layout_below="@+id/imageView"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/settingsButton"
        android:layout_alignEnd="@+id/settingsButton" />

    <Button
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:text="Settings"
        android:id="@+id/settingsButton"
        android:layout_toEndOf="@+id/gameButton"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/gameButton" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="200dp"
        android:id="@+id/imageView"
        android:src="@drawable/sq2"
        android:layout_alignParentTop="true"
        android:layout_alignRight="@+id/settingsButton"
        android:layout_alignEnd="@+id/settingsButton"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="150dp"
        android:layout_height="200dp"
        android:text="Instructions"
        android:id="@+id/instructionsButton"
        android:layout_toStartOf="@+id/gameButton"
        android:layout_alignTop="@+id/gameButton"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:text="Start Game"
        android:id="@+id/gameButton"
        android:layout_below="@+id/textView7"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="63dp" />

</RelativeLayout>
